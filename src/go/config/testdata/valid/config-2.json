{
  "credentials":{
    "production": {
      "awsKeyId": "PROD_KEY_ID",
      "awsSecretKey": "PROD_SECRET_KEY"
    },
    "development": {
      "awsKeyId": "DEV_KEY_ID",
      "awsSecretKey": "DEV_SECRET_KEY"
    },
    "test": {
      "awsKeyId": "TEST_KEY_ID",
      "awsSecretKey": "TEST_SECRET_KEY"
    }
  },
  "constraints": {
    "regions": ["us-west-1"],
    "services": [
      {
        "name": "Test Service 1",
        "minMemory": 4,
        "maxVcpu": 1,
        "focus": "cost",
        "focusWeight": 0.4,
        "instances": {
          "minimum": 1,
          "total": 3
        }
      },
      {
        "name": "Test Service 2",
        "minMemory": 16,
        "maxVcpu": 4,
        "focus": "balanced",
        "focusWeight": 0.9,
        "instances": {
          "minimum": 0,
          "total": 2
        }
      }
    ]
  },
  "api": {
    "port": 54321
  },
  "awsApi": {
    "endpoints": {
      "awsSpotInstanceInfoUrl": "TEST_URL"
    },
    "maxInstancesToFetch": 1000,
    "downloadsDir": "TEST_DOWNLOADS_DIR",
    "freeInstances": false
  },
  "cache": {
    "dirpath": "TEST_CACHE_DIRPATH",
    "defaultLifetime": 200
  }
}