{
  "credentials":{
    "production": {
      "awsKeyId": "PROD_KEY_ID",
      "awsSecretKey": "PROD_SECRET_KEY"
    },
    "development": {
      "awsKeyId": "DEV_KEY_ID",
      "awsSecretKey": "DEV_SECRET_KEY"
    },
    "test": {
      "awsKeyId": "TEST_KEY_ID",
      "awsSecretKey": "TEST_SECRET_KEY"
    }
  },
  "constraints": {
    "regions": ["eu-west-2", "ca-central-1"],
    "services": [
      {
        "name": "Test Service 1",
        "minMemory": 16,
        "maxVcpu": 4,
        "focus": "performance",
        "focusWeight": 0.4,
        "instances": {
          "minimum": 0,
          "total": 2
        }
      }
    ]
  },
  "cache": {
    "dirpath": "TEST_CACHE_DIRPATH",
    "defaultLifetime": 200
  }
}