{
  "constraints": {
    "regions": ["eu-west-2", "ca-central-1"],
    "services": [
      {
        "name": "Test Service 1",
        "minMemory": 16,
        "maxVcpu": 4,
        "focus": "performance",
        "focusWeight": 0.4,
        "instances": {
          "minimum": 0,
          "total": 2
        }
      }
    ]
  },
  "api": {
    "endpoints": {
      "awsSpotInstanceInfoUrl": "TEST_URL"
    },
    "maxInstancesToFetch": 1000,
    "downloadsDir": "TEST_DOWNLOADS_DIR",
    "freeInstances": false
  },
  "cache": {
    "dirpath": "TEST_CACHE_DIRPATH",
    "defaultLifetime": 200
  }
}